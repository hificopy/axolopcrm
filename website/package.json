{
  "name": "axolop-crm",
  "version": "1.0.0-alpha",
  "description": "Axolop CRM - Custom All-in-One HubSpot Competitor for ECOMMERCE, B2B BUSINESS, REAL ESTATE",
  "type": "module",
  "scripts": {
    "postinstall": "npx -y npm-force-resolutions",
    "validate:config": "node scripts/validate-config.js",
    "predev": "npm run validate:config",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:backend\" --names \"VITE,API\" --prefix-colors \"cyan,magenta\"",
    "dev:vite": "vite --host",
    "dev:backend": "nodemon backend/index.js",
    "dev:build": "npm run build && npm run start",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "backend": "nodemon backend/index.js",
    "backend:prod": "node backend/index.js",
    "check-typos": "node scripts/check-component-typos.js",
    "pre-commit": "npm run check-typos && npm run validate:config",
    "start": "node backend/index.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop axolop-crm",
    "pm2:restart": "pm2 restart axolop-crm",
    "pm2:logs": "pm2 logs axolop-crm",
    "pm2:status": "pm2 status",
    "pm2:delete": "pm2 delete axolop-crm",
    "check": "npm run lint && npm run build",
    "lint": "eslint \"frontend/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "format": "prettier -w .",
    "seed": "node scripts/seed-demo-data.js",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:rebuild": "docker-compose -f docker/docker-compose.yml up -d --build",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f"
  },
  "keywords": [
    "crm",
    "axolop",
    "hubspot",
    "sales",
    "email-marketing",
    "automation",
    "ecommerce",
    "b2b",
    "real-estate",
    "typeform",
    "jotform",
    "funnels",
    "manychat",
    "active-campaign",
    "klaviyo",
    "beehive"
  ],
  "author": "Axolop LLC",
  "license": "PROPRIETARY",
